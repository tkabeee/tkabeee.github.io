{
  "name": "tkabeee.github.io",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "webpack --mode production --module-bind 'ts=ts-loader' --resolve-extensions .ts,.js --entry ./src/game.ts --output ./js/game.js",
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^22.1.4",
    "jest": "^22.4.2",
    "ts-jest": "^22.4.1",
    "ts-loader": "^4.0.0",
    "typescript": "^2.7.2",
    "webpack": "^4.0.0",
    "webpack-cli": "^2.0.9"
  }
}
